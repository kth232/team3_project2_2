<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <properties>
        <property name="driver" value="oracle.jdbc.driver.OracleDriver" />
        <property name="url" value="jdbc:oracle:thin:@localhost:1521:EE" />
        <property name="username" value="PROJECT2_2" />
        <property name="password" value="oracle" />
    </properties>
    <environments default="dev">
        <environment id="dev">
            <transactionManager type="JDBC"/>
            <dataSource type="POOLED">
                <property name="driver" value="${driver}"/>
                <property name="url" value="${url}"/>
                <property name="username" value="${username}"/>
                <property name="password" value="${password}"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <mapper resource="org/choongang/grades/mapper/GradeMapper.xml"/>
        <mapper resource="org/choongang/grades/mapper/StudentGradeMapper.xml"/>
        <mapper resource="org/choongang/lecture/mapper/LectureMapper.xml"/>
        <mapper resource="org/choongang/start/member/mapper/MemberMapper.xml"/>
        <mapper resource="org/choongang/studentManagement/mapper/StudentManagementMapper.xml"/>
    </mappers>
</configuration>